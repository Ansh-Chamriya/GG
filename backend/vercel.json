{
    "version": 2,
    "builds": [
        {
            "src": "app/main.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "app/main.py"
        },
        {
            "src": "/docs",
            "dest": "app/main.py"
        },
        {
            "src": "/redoc",
            "dest": "app/main.py"
        },
        {
            "src": "/openapi.json",
            "dest": "app/main.py"
        },
        {
            "src": "/health",
            "dest": "app/main.py"
        },
        {
            "src": "/ready",
            "dest": "app/main.py"
        },
        {
            "src": "^/$",
            "dest": "app/main.py"
        }
    ]
}